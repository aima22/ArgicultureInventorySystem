@model IEnumerable<ArgicultureInventorySystem.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Booking List</h2>

<p>
    @Html.ActionLink("Create New", "Create", "UniversityCommunity", null, new {@class = "btn btn-primary"})
</p>
<table class="table table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id }) | 
            @Html.ActionLink("View Bookings", "CustomerBooking", "Booking", new { id=item.Id }, null)
        </td>
    </tr>
}

</table>
